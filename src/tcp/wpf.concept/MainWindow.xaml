<Window x:Class="cc.isr.Iot.Tcp.Session.Wpf.Concept.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cc.isr.Iot.Tcp.Session.Wpf.Concept"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label x:Name="InstrumentLabel"
               Content="Hello World"
               FontSize="32" />
        <Label
                x:Name="WelcomeLabel"
                Content="Welcome to .NET WPF App"
                FontSize="18" />
        <Button
                x:Name="CounterBtn"
                Content="Click me"
                Click="OnCounterClicked" Margin="0,0,164,117" />
    </StackPanel>
    
</Window>
